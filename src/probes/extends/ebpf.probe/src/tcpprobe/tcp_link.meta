    {
        table_name: "proc_flow_perf",
        entity_name: "proc_flow_perf",
        fields:
        (
            {
                description: "id of process",
                type: "key",
                name: "tgid",
            },
            {
                description: "role",
                type: "key",
                name: "role",
            },
            {
                description: "remote ip",
                type: "key",

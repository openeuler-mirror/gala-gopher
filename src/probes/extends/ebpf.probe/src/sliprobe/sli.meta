version = "1.0.0"
measurements:
(
    {
        table_name: "sli_cpu_container",
        entity_name: "sli_container",
        fields:
        (
            {
                description: "container id",
                type: "key",
                name: "container_id",
            },
            {
                description: "Latency of CPU wait",
                type: "gauge",
                name: "cpu_wait",
            },
            {
                description: "Latency of CPU sleep",
                type: "gauge",
                name: "cpu_sleep",
            },
            {
                description: "Latency of CPU io wait",
                type: "gauge",
                name: "cpu_iowait",
            },
            {
                description: "Latency of CPU block",
                type: "gauge",
                name: "cpu_block",
            },
            {
                description: "Latency of CPU run delay",
                type: "gauge",
                name: "cpu_rundelay",
            },
            {
                description: "Latency of CPU long systime",
                type: "gauge",
                name: "cpu_longsys",
            }
        )
    },
    {
        table_name: "sli_mem_container",
        entity_name: "sli_container",
        fields:
        (
            {
                description: "container id",
                type: "key",
                name: "container_id",
            },
            {
                description: "Latency of memory reclaim",
                type: "gauge",
                name: "mem_reclaim",
            },
            {
                description: "Latency of memory compact",

version = "1.0.0"

measurements:
(
    {
        table_name: "redis_server",
        entity_name: "redis_server",
        fields:
        (
            {
                description: "redis_version",
                type: "Unknown",
                name: "redis_version",
            },
            {
                description: "tcp_port",
                type: "Unknown",
                name: "tcp_port",
            },
            {
                description: "hz",
                type: "Unknown",
                name: "hz",
            },
            {
                description: "io_threads_active",
                type: "Unknown",
                name: "io_threads_active",
            },
        )
    },
    {
        table_name: "redis_clients",
        entity_name: "redis_clients",
        fields:
        (
            {
                description: "connected_clients",
                type: "gauge",
                name: "connected_clients",
            },
            {
                description: "cluster_connections",
                type: "gauge",
                name: "cluster_connections",
            },
        )
    },
    {
        table_name: "redis_memory",
        entity_name: "redis_memory",
        fields:
        (
            {
                description: "used_memory",
                type: "gauge",
                name: "used_memory",
            },
        )
    },
    {
        table_name: "redis_stats",
        entity_name: "redis_stats",
        fields:
        (
            {
                description: "total_connections_received",
                type: "gauge",
                name: "total_connections_received",
            },
            {
                description: "total_commands_processed",
                type: "gauge",
                name: "total_commands_processed",
            },
            {
                description: "instantaneous_ops_per_sec",
                type: "gauge",
                name: "instantaneous_ops_per_sec",
            },
            {
                description: "total_net_input_bytes",
                type: "gauge",
                name: "total_net_input_bytes",
            },
            {
                description: "total_net_output_bytes",
                type: "gauge",
                name: "total_net_output_bytes",
            },
        )
    },

    {
        table_name: "redis_latency",
        entity_name: "redis_latency",
        fields:
        (
            {
                description: "intrinsic_latency",
                type: "gauge",
                name: "intrinsic_latency",
            }
        )
    }
)


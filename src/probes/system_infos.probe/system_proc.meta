version = "1.0.0"

measurements:
(
    {
        table_name: "system_proc",
        entity_name: "proc",
        fields:
        (
            {
                description: "id of process",
                type: "key",
                name: "tgid",
            },
            {
                description: "process startup time",
                type: "label",
                name: "start_time",
            },
            {
                description: "process group id",
                type: "label",
                name: "pgid",
            },
            {
                description: "parent id of process",
                type: "label",
                name: "ppid",
            },
            {
                description: "cmdline of the process",
                type: "label",
                name: "cmdline",
            },
            {
                description: "fd counts of the process",
                type: "gauge",
                name: "fd_count",
            },
            {
                description: "percent of remaining fd resource(%)",
                type: "gauge",
                name: "fd_free_per",
            },
            {
                description: "bytes of proc system call I/O read operation",
                type: "gauge",
                name: "rchar_bytes",
            },
            {
                description: "bytes of proc system call I/O write operation",
                type: "gauge",
                name: "wchar_bytes",
            },
            {
                description: "count of proc system call I/O read operation",
                type: "gauge",
                name: "syscr_count",
            },
            {
                description: "count of proc system call I/O write operation",
                type: "gauge",
                name: "syscw_count",
            },
            {
                description: "bytes of proc read operation from disk",
                type: "gauge",
                name: "read_bytes",
            },
            {
                description: "bytes written to disk by proc (expected)",
                type: "gauge",
                name: "write_bytes",
            },
            {
                description: "bytes written to disk cancelled by proc",
                type: "gauge",
                name: "cancelled_write_bytes",
            },
            {
                description: "size of clean pages in shared memory(kB)",
                type: "gauge",
                name: "shared_clean_size",
            },
            {
                description: "size of dirty pages in shared memory(kB)",
                type: "gauge",
                name: "shared_dirty_size",
            },
            {
                description: "size of clean pages in private memory(kB)",
                type: "gauge",
                name: "private_clean_size",
            },
            {
                description: "size of dirty pages in private memory(kB)",
                type: "gauge",
                name: "private_dirty_size",
            },
            {
                description: "size of memory currently marked as referenced(kB)",
                type: "gauge",
                name: "referenced_size",
            },
            {
                description: "size of memory not immediately freed(kB)",
                type: "gauge",
                name: "lazyfree_size",
            },
            {
                description: "size of data present in proc swap partition(kB)",
                type: "gauge",
                name: "swap_data_size",
            },
            {
                description: "size of proc swap physical partition(kB)",
                type: "gauge",
                name: "swap_data_pss_size",
            },
            {
                description: "count of proc cirtical pagefault",
                type: "gauge",
                name: "minor_pagefault_count",
            },
            {
                description: "count of proc weak pagefault",
                type: "gauge",
                name: "major_pagefault_count",
            },
            {
                description: "time of proc runs in userspace",
                type: "gauge",
                name: "utime_jiffies",
            },
            {
                description: "time of proc runs in kernelspace",
                type: "gauge",
                name: "stime_jiffies",
            },
            {
                description: "time of proc waited-for children in userspace",
                type: "gauge",
                name: "cutime_jiffies",
            },
            {
                description: "time of proc waited-for children in kernelspace",
                type: "gauge",
                name: "cstime_jiffies",
            },
            {
                description: "Dynamic priority of proc",
                type: "gauge",
                name: "priority",
            },
            {
                description: "Static priority of proc",
                type: "gauge",
                name: "nice",
            },
            {
                description: "Number of threads in the group",
                type: "gauge",
                name: "num_threads",
            },
            {
                description: "byte of proc's virtual address space(B)",
                type: "gauge",
                name: "vm_size",
            },
            {
                description: "byte of proc's physical address space(B)",
                type: "gauge",
                name: "pm_size",
            },
            {
                description: "util of proc's physical address used(%)",
                type: "gauge",
                name: "pm_util",
            },
            {
                description: "ID of the CPU where proc last run",
                type: "gauge",
                name: "cpu",
            }
        )
    }
)

